{
    "name": "saadmughal/admin-auth-php",
    "description": "Admin authentication package for Laravel with Firebase notifications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saad Mughal",
            "email": "saadey7@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mughal\\AdminAuth\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "kreait/firebase-php": "^5.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0|^13.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "saadmughal/admin-auth-php"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan adminauth:install || true"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}